{
  "name": "typescript-node-api",
  "version": "1.0.0",
  "description": "Typescript Node API",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "yarn build && node dist/src/index.js",
    "start-dev": "ts-node-dev src/index.ts",
    "lint": "eslint ./src ./test --ext .ts",
    "lint-fix": "eslint ./src ./test --ext .ts --fix",
    "test": "echo \"Error: no test specified\" && exit 1",
    "test:functional": "jest --projects ./test --forceExit --runInBand",
    "test:unit": "jest",
    "style:check": "prettier --check src/**/*.ts test/**/*.ts",
    "style:fix": "prettier --write src/**/*.ts test/**/*.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wallefaquino/typescript-node-api.git"
  },
  "keywords": [
    "nodejstypescriptapi"
  ],
  "author": "Wallef Aquino",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/wallefaquino/typescript-node-api/issues"
  },
  "homepage": "https://github.com/wallefaquino/typescript-node-api#readme",
  "dependencies": {
    "@overnightjs/core": "^1.7.4",
    "axios": "^0.19.2",
    "body-parser": "^1.19.0",
    "config": "^3.3.6",
    "express": "^4.17.1",
    "module-alias": "^2.2.2"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/config": "^0.0.39",
    "@types/express": "^4.17.6",
    "@types/jest": "^25.2.2",
    "@types/module-alias": "^2.0.0",
    "@types/node": "^13.13.2",
    "@types/supertest": "^2.0.9",
    "@typescript-eslint/eslint-plugin": "^2.29.0",
    "@typescript-eslint/parser": "^2.29.0",
    "eslint": "^6.8.0",
    "jest": "^26.0.1",
    "prettier": "^2.3.2",
    "supertest": "^4.0.2",
    "ts-jest": "^26.0.0",
    "ts-node-dev": "^1.0.0-pre.44",
    "typescript": "^3.8.3"
  }
}
